{
  "name": "lanyao-terminal",
  "version": "1.0.0",
  "description": "澜鳐控制终端 - 基于 Electron + Vue 的物联网设备控制平台",
  "main": "dist-electron/electron/main.js",
  "scripts": {
    "dev": "vite --config build/vite.config.ts",
    "build": "vite build --config build/vite.config.ts",
    "preview": "vite preview --config build/vite.config.ts",
    "electron:dev": "tsc -p tsconfig.node.json && vite build --config build/vite.config.ts && electron dist-electron/electron/main.js",
    "electron:build": "vite build --config build/vite.config.ts && electron-builder"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "element-plus": "^2.3.0",
    "pinia": "^2.1.0",
    "sqlite3": "^5.0.11",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "winston": "^3.10.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.6.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-electron": "^0.29.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "electron",
    "vue",
    "iot",
    "terminal"
  ],
  "author": "",
  "license": "MIT"
}
